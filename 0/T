FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 573;

boundaryField
{
    inlet
    {
        type            fixedValue;
		value			uniform 573;
    }
    outlet
    {
        type    zeroGradient;
    }
    Wall_side
    {
        type    zeroGradient;
    }
    Wall_out
    {
        type    zeroGradient;
    }
    Wall_inside
    {
        type    zeroGradient;
    }



}


